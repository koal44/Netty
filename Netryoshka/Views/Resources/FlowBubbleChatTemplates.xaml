                    xmlns:local="clr-namespace:Netryoshka">

  <DataTemplate x:Key="AppNullDataTemplate">
    <local:FlowChatBubble />
  </DataTemplate>
  <DataTemplate x:Key="AppHttpDataTemplate">
    <local:TcpHexFlowChatBubble />
  </DataTemplate>
  <DataTemplate x:Key="AppHttpsDataTemplate">
    <local:TcpHexFlowChatBubble />
  </DataTemplate>
  <DataTemplate x:Key="AppLengthPrefixDataTemplate">
    <local:TcpHexFlowChatBubble />
  </DataTemplate>
  <DataTemplate x:Key="TcpHexDataTemplate">
    <local:TcpHexFlowChatBubble />
  </DataTemplate>
  <DataTemplate x:Key="TcpAsciiDataTemplate">
    <local:TcpAsciiFlowChatBubble />
  </DataTemplate>
  <DataTemplate x:Key="IpDataTemplate">
    <local:TcpHexFlowChatBubble />
  </DataTemplate>
  <DataTemplate x:Key="EthDataTemplate">
    <local:EthFlowChatBubble />
  </DataTemplate>
  <DataTemplate x:Key="FrameDataTemplate">
    <local:FrameFlowChatBubble />
  </DataTemplate>
