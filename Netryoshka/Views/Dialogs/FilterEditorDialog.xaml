<UserControl x:Class="Netryoshka.Views.Dialogs.FilterEditorDialog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Netryoshka.Views.Dialogs"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             Margin="0"
             Padding="0"
             d:DesignHeight="100"
             d:DesignWidth="300"
             mc:Ignorable="d">
    <Grid>
        <TextBox Name="CustomFilterTextBox"
                 MinWidth="300"
                 MinHeight="100"
                 Text="{Binding CustomFilter, RelativeSource={RelativeSource AncestorType=UserControl}}" />
    </Grid>
</UserControl>
